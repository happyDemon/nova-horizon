{
    "name": "happydemon/nova-horizon",
    "description": "Horizon statistics in Nova",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0",
        "laravel/horizon": "^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {
            "HappyDemon\\NovaHorizon\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "HappyDemon\\NovaHorizon\\NovaHorizonServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
